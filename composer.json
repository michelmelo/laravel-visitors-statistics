{
    "name": "michelmelo/laravel-visitors-statistics",
    "description": "Simple visitor tracker and statistics package for Laravel 5 that can be used for dashboard graphs.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Melo",
            "email": "eu@michelmelo.pt"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-zlib": "*",
        "illuminate/http": "~10.0",
        "illuminate/database": "~10.0",
        "illuminate/routing": "~10.0",
        "illuminate/support": "~10.0",
        "piwik/device-detector": "^3.11",
        "maxmind-db/reader": "^1.4"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "MichelMelo\\LaravelVisitorsStatistics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichelMelo\\LaravelVisitorsStatistics\\Tests\\": "tests/"
        }
    }
}
